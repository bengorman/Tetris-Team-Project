#ifndef U_BLOCK_H_
#define U_BLOCK_H_


class U_Block : public FallingBlock{

    /*
     | 0 1 0 |
     | 0 1 0 |
     | 1 1 0 |
     */

public:
    U_Block() : FallingBlock(8) { // Configure original position of block and create grid
        // Default position
        grid[0][1] = 1;
        grid[1][1] = 1;
        grid[2][1] = 1;
        grid[2][0] = 1;
    }
    short getType() {
        	return 8;
	}
    ~U_Block() {};
};



#endif /* U_BLOCK_H_ */
